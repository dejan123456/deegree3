
<?xml version="1.0"?>
<!-- AspectJ load-time weaving config file to install common Spring aspects. -->

<aspectj>
  <weaver options="-showWeaveInfo">
          <include within="org..*" />
          </weaver>

  <aspects>
    <aspect
      name="org.springframework.security.access.intercept.aspectj.aspect.AspectJMethodSecurityInterceptor" />
  </aspects>

</aspectj>

